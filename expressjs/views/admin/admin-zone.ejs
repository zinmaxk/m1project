                <div class="card">
                    <div class="card-body">
                        <label class="control-label">First name</label>
                        <input class="form-control form-control-subject" placeholder="First name"
                               required>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-6 mx-auto">
            <div class="col-md-12" style="padding-bottom: 20px;">
                <div class="card">
                    <div class="card-body">
                        <label class="control-label">Last name</label>
                        <input class="form-control form-control-subject" placeholder="Last name"
