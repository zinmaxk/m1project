<!DOCTYPE html>
<html lang="en">

<head>
    <% include ./partials/head %>
</head>

<body>
<div class="body-inner">
    <!-- Header start -->
    <% include ./partials/header %>
    <!--/ Header end -->
    <!-- Banner start -->
    <% include ./partials/banner %>
    <!-- Banner end -->
    <!-- ts Content start -->
    <section class="ts-schedule">
        <div class="container" style="padding-bottom: 30px">
            <div class="row">
                <div class="col-lg-12">

                    <h2 class="section-title">
                       08/08/2019

                    </h2>

                </div>
                <!-- col end-->



            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="tab-content schedule-tabs">
                        <div role="tabpanel" class="tab-pane active">
                            <% scheduleList.forEach(function(schedule) { %>
                                <div class="schedule-listing">
                                    <div class="schedule-slot-time">
                                        <span> <%= schedule.time %></span> Yoga Class
                                    </div>
                                    <div class="schedule-slot-info">
                                        <a href="#">
                                            <img class="schedule-slot-speakers" src="<%= schedule.thumbnail %>" alt="">
                                        </a>
                                        <div class="schedule-slot-info-content card-body">
                                            <h3 class="schedule-slot-title">
                                                <a href="#">
                                                    <%= schedule.title %>
                                                </a>
                                                <strong>@ <%= schedule.location %></strong>
                                            </h3>

                                            <p>
                                                <%= schedule.brief %>
                                            </p>
                                            <div style="padding-top: 20px;" class="text-right">
                                                <a href="/owner/owner-booking-form" class="btn text-right" > Book this class </a>
                                            </div>

                                        </div>
                                        <!--Info content end -->
                                    </div>
                                    <!-- Slot info end -->
                                </div>
                            <% }); %>
                            <!--schedule-listing end -->
                            <div class="schedule-listing-btn">
                                <a href="#" class="btn">More Details</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- footer start-->
    <% include ./partials/footer %>
    <!-- Body inner end -->
</div>
<% include ./partials/footer-script %>
</body>

</html>