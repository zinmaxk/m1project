<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../partials/head %>
</head>

<body>
<div class="body-inner">
    <!-- Header start -->
    <% include ../partials/header %>
    <!--/ Header end -->
    <!-- Banner start -->
    <% include ../partials/banner %>
    <!-- Banner end -->
    <!-- ts Content start -->
    <!-- Center management -->
    <section>
        <div class="container">
            <div class="row text-center">
                <div class="col-lg-4 mx-auto">
                    <h2 class="section-title text-center">
                        <span>Follow actions below </span>
                        Center Management
                    </h2>
                </div>
            </div>
            <!--/ Title row end -->
            <div class="sponsors-wrap">
                <div class="row text-center">
                    <div class="col-lg-4 mx-auto">
                        <h2 class="section-title text-center">
                            <a href="/owner/owner-centercreation" class="btn"> Create new Center </a>
                    </div>
                </div>
            </div>
        </div>

        <section class="ts-contact-form">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 mx-auto">
                        <h2 class="section-title text-center">
                            <span> List of center </span>
                            Center
                        </h2>
                    </div><!-- col end-->
                </div>

                <div class="row">
                    <div class="col-lg-8 mx-auto">
                        <div class="error-container"></div>
                        <div class="row">
                            <% centers.forEach(function(center) { %>
                                <div class="col-md-6" style="padding-bottom: 30px;">
                                    <div class="card">
                                        <div class="card-body">
                                            <p>
                                                <a href="/owner/owner-centerdetail/<%= center.id %>"> <%= center.name %> </a>
                                            </p>
                                            <p> Time Open: <%= center.openTime %> </p>
                                            <p> Time Close: <%= center.closeTime %> </p>
                                            <p> Center City: <%= center.city %> </p>
                                        </div>
                                    </div>
                                </div>
                            <% }) %>
                        </div>
                    </div>
                </div>


            </div>
</div>
</div>
<div class="speaker-shap">
    <img class="shap1" src="images/shap/home_schedule_memphis2.png" alt="">
</div>
</section>


<!-- footer start-->
<% include ../partials/footer %>
<!-- Body inner end -->
</div>
<% include ../partials/footer-script %>
</body>

</html>